<%= form_with model: @user do |form| %>
    <%= form.label :handle %>
    <%= form.text_field :handle %>

    <%= form.label :display_name %>
    <%= form.text_field :display_name %>

    <%= form.label :bio %>
    <%= form.text_field :bio %>

    <%= form.label :born_at %>
    <%= form.text_field :born_at %>

    <%= form.submit %>
<% end %>